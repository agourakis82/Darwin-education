{
  "name": "darwin-education",
  "version": "1.5.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "overrides": {
      "@types/react": "^19.0.0",
      "@types/react-dom": "^19.0.0"
    }
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "deploy:smoke": "tsx scripts/deploy_smoke.ts",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "etl": "node --import tsx infrastructure/supabase/seed/etl.ts",
    "seed:medical-content": "pnpm --filter @darwin-education/web exec env TSX_TSCONFIG_PATH=../../darwin-MFC/tsconfig.json node --import tsx ../../scripts/import_medical_content.ts",
    "audit:medical-content": "env TSX_TSCONFIG_PATH=darwin-MFC/tsconfig.json node --import tsx scripts/audit_medical_content.ts",
    "validate:jsonld": "env TSX_TSCONFIG_PATH=darwin-MFC/tsconfig.json node --import tsx scripts/validate_jsonld.ts",
    "scaffold:medical-fullcontent": "env TSX_TSCONFIG_PATH=darwin-MFC/tsconfig.json node --import tsx scripts/scaffold_disease_fullcontent_drafts.ts",
    "harvest": "pnpm --filter @darwin-education/shared harvest",
    "harvest:test": "pnpm --filter @darwin-education/shared harvest:test",
    "harvest:sources": "pnpm --filter @darwin-education/shared harvest:sources",
    "brand:score-logo": "node --import tsx scripts/brand/score_logo_candidates.ts",
    "db:push": "bash deploy.sh",
    "db:login": "supabase login",
    "db:link": "cd infrastructure/supabase && supabase link --project-ref jpzkjkwcoudaxscrukye",
    "db:status": "cd infrastructure/supabase && supabase status"
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "tsx": "^4.21.0",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.91.1",
    "pg": "^8.18.0",
    "postgres": "^3.4.8"
  }
}
