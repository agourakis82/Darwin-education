{
  "name": "darwin-education",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "overrides": {
      "@types/react": "^19.0.0",
      "@types/react-dom": "^19.0.0"
    }
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "etl": "tsx infrastructure/supabase/seed/etl.ts",
    "harvest": "pnpm --filter @darwin-education/shared harvest",
    "harvest:test": "pnpm --filter @darwin-education/shared harvest:test",
    "harvest:sources": "pnpm --filter @darwin-education/shared harvest:sources"
  },
  "devDependencies": {
    "prettier": "^3.2.0",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
